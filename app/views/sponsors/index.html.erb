<h1>Listing sponsors</h1>

<table>
  <tr>
    <th>Name</th>
    <th>Verified</th>
    <th>Review</th>
    <th>University</th>
    <th>Job</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @sponsors.each do |sponsor| %>
  <tr>
    <td><%= sponsor.name %></td>
    <td><%= sponsor.verified %></td>
    <td><%= sponsor.review %></td>
    <td><%= sponsor.university %></td>
    <td><%= sponsor.job %></td>
    <td><%= link_to 'Show', sponsor %></td>
    <td><%= link_to 'Edit', edit_sponsor_path(sponsor) %></td>
    <td><%= link_to 'Destroy', sponsor, confirm: 'Are you sure?', method: :delete %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'New Sponsor', new_sponsor_path %>
